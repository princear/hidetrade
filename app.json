{
  "expo": {
    "name": "hidetrade",
    "slug": "hidetrade",
    "version": "16.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/301.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.HideTrade.HideTrade",
      "buildNumber": "1.0.35"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/300.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.HideTrade.HideTrade",
      "versionCode": 19,
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          },
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "owner": "eastsons",
    "extra": {
      "eas": {
        "projectId": "246beb0c-6a55-4780-928f-eb6e958e5fd5"
      }
    }
  }
}
